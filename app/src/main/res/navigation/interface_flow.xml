<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/interface_flow"
    app:startDestination="@id/mainActivity">

    <activity
        android:id="@+id/mainActivity"
        android:name="com.snowbear.happylottery.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
    <activity
        android:id="@+id/dataActivity"
        android:name="com.snowbear.happylottery.DataActivity"
        android:label="activity_data"
        tools:layout="@layout/activity_data" />
    <activity
        android:id="@+id/setActivity"
        android:name="com.snowbear.happylottery.SetActivity"
        android:label="SetActivity" />
    <activity
        android:id="@+id/lotteryResultActivity"
        android:name="com.snowbear.happylottery.LotteryResultActivity"
        android:label="activity_lottery_result"
        tools:layout="@layout/activity_lottery_result" />
    <activity
        android:id="@+id/dataItemSet"
        android:name="com.snowbear.happylottery.DataItemSet"
        android:label="activity_data_item_set"
        tools:layout="@layout/activity_data_item_set" />
</navigation>