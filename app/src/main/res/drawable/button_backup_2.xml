<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <rotate
            android:fromDegrees="-45"
            android:toDegrees="45"
            android:pivotX="270%"
            android:pivotY="70%" >
            <shape
                android:shape="rectangle" >
                <stroke android:color="@color/MangoOrange" android:width="1dp"/>
                <solid
                    android:color="@color/MangoOrange" />
            </shape>
        </rotate>
    </item>
</layer-list>